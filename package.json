{
  "name": "mopgomglobal-admin",
  "version": "1.0.0",
  "description": "Mopgomglobal Youth Registration System - Admin Dashboard",
  "private": true,
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "dev:simple": "next dev -H 0.0.0.0 -p 3000",
    "build": "prisma generate && next build",
    "build:production": "SKIP_TYPE_CHECK=true prisma generate && next build",
    "render-build": "npm install && prisma generate && prisma migrate deploy && npm run build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",

    "setup": "node scripts/setup-dev.js",
    "setup:admin": "npx tsx scripts/create-super-admin.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "clean": "rimraf .next tsconfig.tsbuildinfo",
    "clean:cache": "rimraf .next/cache",
    "fresh": "npm run clean && npm run dev"
  },
  "dependencies": {
    "@prisma/client": "^6.11.0",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.460.0",
    "next": "^15.3.3",
    "nodemailer": "^6.9.16",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.15",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.1",
    "@types/nodemailer": "^6.4.15",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.15.0",
    "eslint-config-next": "^15.3.3",
    "postcss": "^8.5.1",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "youth",
    "registration",
    "admin",
    "dashboard",
    "nextjs",
    "typescript",
    "prisma",
    "tailwindcss"
  ],
  "author": "Mopgomglobal Team",
  "license": "MIT"
}
